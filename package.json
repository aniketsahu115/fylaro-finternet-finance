{
  "name": "fylaro-finternet-finance",
  "description": "Invoice Tokenization Platform on BNB Smart Chain with Real-time Trading and Credit Scoring",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "start:dev:simple": "npm run backend:dev & npm run dev",
    "start:dev:win": "dev-start.bat",
    "start:dev:unix": "./dev-start.sh",
    "deploy": "./deploy.sh",
    "deploy:prod": "./deploy.sh --production",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf node_modules backend/node_modules dist backend/dist .backend.pid .frontend.pid",
    "setup": "npm install && cd backend && npm install",
    "health": "curl -f http://localhost:3001/health || echo 'Backend not running'",
    "compile": "npx hardhat compile",
    "deploy:local": "npx hardhat run scripts/deploy-local.js --network localhost",
    "deploy:testnet": "npx hardhat run scripts/deploy.js --network bscTestnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network bsc",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "verify:testnet": "npx hardhat run scripts/verify.js --network bscTestnet",
    "verify:mainnet": "npx hardhat run scripts/verify.js --network bsc",
    "hardhat:node": "npx hardhat node",
    "hardhat:console": "npx hardhat console",
    "contracts:size": "npx hardhat size-contracts",
    "contracts:clean": "npx hardhat clean"
  },
  "dependencies": {
