    "eslint-plugin-react-refresh": "^0.4.20",
    "ethers": "^6.15.0",
    "globals": "^15.15.0",
    "hardhat": "^3.0.4",
    "lovable-tagger": "^1.1.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19"
  }
}
